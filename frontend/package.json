    "@types/chai": "^5.0.0",
    "@types/jsdom": "^21.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^22.0.0",
    "@types/selenium-webdriver": "^4.1.22",
    "@types/sinon": "^17.0.3",
    "@types/splitpanes": "^2.2.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vitest/coverage-v8": "^2.0.0",
